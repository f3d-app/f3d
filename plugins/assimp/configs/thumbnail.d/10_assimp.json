[
  {
    "match-type": "glob",
    "match": "*.{fbx,dae,off,dxf,x,3mf}",
    "options": {
      "load-plugins": "assimp"
    }
  },
  {
    "match-type": "glob",
    "match": "*.{off,3mf}",
    "options": {
      "up": "+Z",
      "camera-direction": "-1,1,-0.5"
    }
  },
  {
    "match-type": "glob",
    "match": "*.dxf",
    "options": {
      "up": "-Z",
      "camera-direction": "1,-1,0.5"
    }
  }
]
