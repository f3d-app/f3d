<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/vnd.exodus">
    <comment>Exodus 2 file</comment>
    <glob pattern="*.ex2"/>
    <glob pattern="*.exo"/>
    <glob pattern="*.e"/>
    <glob pattern="*.g"/>
  </mime-type>
  <mime-type type="application/vnd.vtkhdf">
    <comment>VTKHDF file</comment>
    <glob pattern="*.vtkhdf"/>
  </mime-type>
  <mime-type type="application/netcdf">
    <comment>NetCDF file</comment>
    <glob pattern="*.nc"/>
    <glob pattern="*.cdf"/>
    <glob pattern="*.ncdf"/>
  </mime-type>
</mime-info>
