{
  ".*(dcm|nrrd|mhd|mha|vt.)":
  {
    "bar": true
  },
  ".*(dcm|nrrd|mhd|mha|vti)":
  {
    "volume": true
  },
  ".*(3ds|stl)":
  {
    "up": "+Z",
    "camera-direction": "-1,1,-1"
  },
  ".*(tif|tiff)":
  {
    "up": "-Y",
    "comp": "-2",
    "camera-direction": "-1,1,1"
  },
  ".*(ply)":
  {
    "comp": "-2",
    "translucency-support": true
  }
}
