[
{
  "match": ".*(dcm|nrrd|mhd|mha|vti)",
  "options":
  {
    "scalar-coloring": true,
    "volume": true
  }
},
{
  "match": ".*(3ds|stl)",
  "options":
  {
    "up": "+Z",
    "camera-direction": "-1,1,-0.5"
  }
},
{
  "match": ".*(stl|gml|pts|vt.)",
  "options":
  {
    "scalar-coloring": true
  }
},
{
  "match": ".*(ply)",
  "options":
  {
    "scalar-coloring": true,
    "comp": "-2",
    "translucency-support": true
  }
},
{
  "match": ".*(splat)",
  "options":
  {
    "point-sprites": true,
    "point-sprites-size": 1,
    "point-sprites-type": "gaussian",
    "scalar-coloring": true,
    "coloring-array": "color",
    "comp": "-2",
    "up": "-Y",
    "camera-position": "0,1,-5.2",
    "camera-focal-point": "0,1,0",
    "tone-mapping": false,
    "grid": false,
    "anti-aliasing": false,
    "translucency-support": false
  }
}
]
