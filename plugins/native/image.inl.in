bool canRead(vtkResourceStream* stream) const override
{
  std::string hint;
  bool ret = vtkF3DImageImporter::CanReadFile(stream, hint);
  // clang-format off
  ret &= (hint == "@_image_format@");
  // clang-format on
  return ret;
}

void applyCustomImporter(vtkImporter* importer, const std::string& vtkNotUsed(fileName),
  vtkResourceStream* stream) const override
{
  // Set hint
  vtkF3DImageImporter* imageImporter = vtkF3DImageImporter::SafeDownCast(importer);
  // clang-format off
  imageImporter->SetImageHint("@_image_format@");
  // clang-format on
}
